schema test {
  document test {
    field id type long {
      indexing: attribute
      attribute: fast-search
    }
    field f1 type weightedset<long> {
      indexing: attribute
      attribute: fast-search
    }
    field s1 type weightedset<string> {
      indexing: attribute
      attribute: fast-search
    }
    field payload_array_byte type array<byte> {
      indexing: attribute
    }
    field payload_array_long type array<long> {
      indexing: attribute
    }
    field payload_raw type raw {
      indexing: attribute
    }
  }
  field f1_hash type weightedset<long> {
    indexing: input f1 | attribute
    attribute: fast-search
    dictionary: hash
  }
  field s1_cased type weightedset<string> {
    indexing: input s1 | attribute
    attribute: fast-search
    dictionary {
      btree
      cased
    }
    match:cased
  }
  field s1_hash type weightedset<string> {
    indexing: input s1 | attribute
    attribute: fast-search
    dictionary {
      hash
      cased
    }
    match:cased
  }
  document-summary minimal {
    summary id type long { source: id }
  }
  document-summary raw_bytes {
    summary id type long { source: id }
    summary payload_raw type raw { source: payload_raw }
  }
  document-summary array_byte {
    summary id type long { source: id }
    summary payload_array_byte type array<byte> { source: payload_array_byte }
  }
  document-summary array_long {
    summary id type long { source: id }
    summary payload_array_long type array<long> { source: payload_array_long }
  }
}
