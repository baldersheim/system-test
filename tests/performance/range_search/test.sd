schema test {
  document test {
    field id type int {
      indexing: attribute | summary
    }
    field values type array<int> {
      indexing: attribute | summary
      attribute: fast-search
      rank: filter
    }
    field filter type array<int> {
      indexing: attribute | summary
      attribute: fast-search
      rank: filter
    }
  }
  document-summary minimal {
    summary id type int {}
  }
}
