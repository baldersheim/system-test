schema genfeed {
    fieldset default {
        fields: title
    }
    document genfeed {
        field title type string {
            indexing: index | summary
        }
        field body type string {
            indexing: summary
        }
        field tag type string {
            indexing: summary | attribute
        }
        field seqno type int {
            indexing: summary | attribute
        }
        field id type string {
            indexing: summary | attribute
        }
    }
}
