schema proximitygap {

  document proximitygap {

    field id type string {
      indexing: summary | index
    }

    field f1 type string {
      indexing: summary | index
    }

    field f2 type string {
      indexing: summary | index
    }

  }
  fieldset default {
    fields: f1, f2
  }

  rank-profile only-proximity inherits default {
    first-phase {
	expression: nativeProximity
    }
  }

}
