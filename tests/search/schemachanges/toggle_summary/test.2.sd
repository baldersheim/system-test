schema test {
  document test {
    field f1 type string {
      indexing: index | summary
    }
    field f2 type int {
      indexing: attribute
    }
  }
  rank-profile rp1 inherits default {
    first-phase {
      expression: if(isNan(attribute(f2)) == 1.0, 0.0, attribute(f2))
    }
  }
}
