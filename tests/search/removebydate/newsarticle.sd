schema newsarticle {

  document newsarticle {

    field title type string {
      indexing: summary | index
    }

    field pubdate type long {
      indexing: summary | attribute
    }
  }

  rank-profile default {
    first-phase {
      expression: attribute(pubdate)
    }
  }

}
