schema test {
  document test {
    field features type weightedset<string> {
      indexing: attribute | summary
    }
    field filter type string {
      indexing: attribute | summary
    }
  }
  rank-profile default {
    first-phase {
      expression: nativeDotProduct(features)
    }
  }
}
