search test {
  document test {
    field content type string {
      indexing: index | summary
      index: enable-bm25
    }
    field contenta type array<string> {
      indexing: index | summary
      index: enable-bm25
    }
  }
  rank-profile default {
    first-phase {
      expression {
        bm25(content) + bm25(contenta)
      }
    }
  }
}
