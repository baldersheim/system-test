schema foo {
  document {
    struct mystruct {
      field name type string {}
      field age type int {}
    }
    field f1 type mystruct {
      indexing: summary
    }
    field f2 type array<mystruct> {
      indexing: summary
      struct-field name {
        indexing: attribute
      }
      struct-field age {
        indexing: attribute
      }
    }
  }
}
