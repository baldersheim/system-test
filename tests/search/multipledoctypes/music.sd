schema music {
  document music inherits common {
    field artist type string {
      indexing: index | summary
    }
  }
  fieldset default {
    fields: artist
  }
  rank-profile year inherits default {
    first-phase { expression: attribute(year) + 0.0 * debugWait(1.2, 0.0) }
  }
}
