schema video {
  document video inherits common {
    field director type string {
      indexing: index | summary
    }
  }
  fieldset default {
    fields: director
  }
  rank-profile year inherits default {
    first-phase { expression: attribute(year) + 0.0 * debugWait(1.0, 0.0)}
  }
}
