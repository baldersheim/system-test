schema dbzero {
  document dbzero {
    field sint type int {
      indexing: attribute | summary
    }
    field sfloat type float {
      indexing: attribute | summary
    }
    field wsint type weightedset<int> {
      indexing: attribute | summary
    }
    field wsstr type weightedset<string> {
      indexing: attribute | summary
    }
  }
}
