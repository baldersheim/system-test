schema test {
  document test {
    field text type string {
      indexing: index | summary
      index: enable-bm25
    }
    field pos type tensor(x[2]) {
      indexing: attribute | summary
    }
  }
  rank-profile weakand {
    first-phase {
      expression: bm25(text)
    }
    second-phase {
      expression: bm25(text)
    }
  }
  rank-profile nns {
    inputs {
      query(query_pos) tensor<double>(x[2])
    }
    first-phase {
      expression: 100 - distance(label,nns)
    }
    second-phase {
      expression: 100 - distance(label,nns)
    }
    match-features {
      itemRawScore(nns)
    }
  }
}
