schema blogpost {
  document blogpost {
    field title type string {
      indexing: summary | index
    }
    field author type int {
      indexing: summary | attribute
    }
    field likes type array<int> {
      indexing: summary | attribute
    }
    field timestamp type long {
      indexing: summary | attribute
    }
  }

}
