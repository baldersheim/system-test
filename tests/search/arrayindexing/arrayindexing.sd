schema arrayindexing {

  document arrayindexing {

    field songtitles type array<string> {
      indexing: index | summary
    }
 
    field weightedtitles type weightedset<string> {
      indexing: index | summary
    }

  }
  fieldset default {
    fields: songtitles, weightedtitles
  }
  fieldset songtitle {
    fields: songtitles
  }

}
