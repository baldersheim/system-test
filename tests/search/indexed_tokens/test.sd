schema test {
  document test {
    field stext type string {
      indexing: index | summary
    }
    field atext type array<string> {
      indexing: index | summary
    }
    field wtext type weightedset<string> {
      indexing: index | summary
    }
  }
  document-summary tokens {
    summary stext_tokens type string {
      source: stext
      tokens
    }
    summary atext_tokens type array<string> {
      source: atext
      tokens
    }
    summary wtext_tokens type weightedset<string> {
      source: wtext
      tokens
    }
    from-disk
  }
}
