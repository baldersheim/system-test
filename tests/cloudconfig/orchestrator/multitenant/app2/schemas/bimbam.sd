schema bimbam {
  
  fieldset default {
    fields: title
  }
  document bimbam {
    field title type string {
      indexing: index | summary
    }
    field weight type float {
      indexing: summary | attribute
    }
  }

  rank-profile default {
    first-phase {
      expression: 100 * nativeRank + attribute(weight)
    }
  }

}

