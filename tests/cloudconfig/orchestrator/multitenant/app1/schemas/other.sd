schema other {
  
  fieldset default {
    fields: description
  }
  document other {
    field description type string {
      indexing: index | summary
    }
    field popularity type int {
      indexing: summary | attribute
    }
  }

  rank-profile default {
    first-phase {
      expression: 100 * nativeRank + attribute(popularity)
    }
  }

}

